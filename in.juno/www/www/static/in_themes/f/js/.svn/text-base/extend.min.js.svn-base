$(function(){
	$("#search_buildings_form").on("submit",function(){
		$("#search_building_btn").click();
		return false;
	})
	$("#search_building_btn").click(function(){
		
		var area = $("#search_building_area").val();
		var price = $("#search_building_price").val();
		var house_type = $("#search_building_type").val();
		var loop = $("#search_building_loop").val();
		var keyword = $("#keywords").val() ?   $("#keywords").val() : ''  ;
		
		var url = "/search/building/";
		
		location.href=url+area+"/"+price+"/"+house_type+"/"+loop+"/0/0/0/0/0/"+keyword;
	})
})
