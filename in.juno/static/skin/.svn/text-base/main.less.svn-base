}
#container-page {
	padding: 77px 0 0;
	.container {
		width: 1000px;
		margin: 0px auto;
		//background: url(@channel-back-image) -230px top repeat-y;
		.main {
			float: right;
			width: 810px;
		}
		.main-l {
			float: left;
			width: 560px;
		}
		.main-r {
			float: right;
			width: 230px;
		}
	}
	.container-fluid {
		.clearfix();
		width: 1000px;
		margin: 0px auto;
