.praise {
	background: url("icon_praise.png") no-repeat scroll 0 0 transparent;
    cursor: pointer;
    display: inline-block;
	.ie7-inline-block();
    height: 20px;
    text-indent: -9999px;
    width: 20px;
	&.on {
	    background-position: 0 -40px;
	}
}
