.push {
	border-right: 1px #e6e6e6 solid;
	border-bottom: 1px #e6e6e6 solid;
	background: #fff;
	.tit {
		height: 33px;
		line-height: 33px;
		background: #eee;
		padding-left: 15px;
        border-top: 2px solid #d52f2f;
		.clearfix();
		h2 {
			padding: 0px;
			margin: 0 10px 0 0;
			font-weight: normal;
			font-size: 18px;
			line-height: 33px;
			float: left;
			.cleartype();
		}
		.say {
			width: 68px;
			height: 25px;
			margin: 4px 0 0 0;
			background: url("channel-style.png") 0 -160px no-repeat;
			display: block;
			float: left;
			color: #fff;
			font-weight: bold;
			font-size: 14px;
			text-align: center;
			text-decoration: none;
			line-height: 28px;
			padding-left: 10px;
		}
		.tab {
			float: right;
			width: 180px;
			height: 33px;
			a {
				display: block;
				width: 90px;
				height: 40px;
				line-height: 30px;
				text-align: center;
				color: #333;
				font-size: 18px;
				float: left;
				text-decoration: none;
				.cleartype();
				&.current {
					color: #fff;
					background: url('tab-bg.png') no-repeat;
				}
			}
		}
		.breadcrumbs {
			float: right;
		}
	}
	.con {
		//padding-bottom: 45px;
	}
}

/* 二维码 */
.item-qrcode {
 	display: inline-block;
 	.ie7-inline-block();
 	background: url("icon_qrcode.png") center center no-repeat #fff;
	width: 17px;
	height: 17px;
	line-height:16px;
	text-indent: -9999px;
	overflow: hidden;
	border: 1px solid #f0f0f0;
	border-right: 1px solid #e1e1e1;
	border-bottom: 1px solid #e1e1e1;
	&:hover {
		border: 1px solid #e1e1e1;
	}
}

.qr-popover {
	width: 200px;
	.thumb {
		 text-align: center;
		 img {
		 	width: 200px !important;
		 	height: 200px !important;
		 	border-radius: 0 !important;
		 	float: none !important;
		 	margin: 0 !important;
		 }
	}
	.notic {
		 color: #aaa;
		 font-size: 14px;
		 font-weight: normal;
		 padding: 10px;
	}
}

#place-map-container {
	img {
		max-width: none;
	}
}
#tip-post{
	.textarea-edit{
		position:relative;
		.txt_tip{
			position:absolute;
			line-height:80px;
			text-align:center;
			width:100%;
			a{
				color:#7FB612;
			}
		}
	}
} 
#content{
	table,table td{
		border:1px solid #DDDDDD;
	}
}
