<!--{template web/header}-->

<div id="container" class="container">
	<div class="section fixed clearfix">
		<div class="main">
			<div class="inner clearfix">
				<div class="main-header">
					<h2>个人设置</h2>
				</div>
				<div class="main-content">
					<!--{template tit}-->
					<div class="con">
						<form action="/user_email/" method="post" class="form-horizontal" id="email-form">
							<div class="control-group">
								<label class="control-label" for=""><span class="warn">*</span> 登录密码：</label>
								<div class="controls">
									<input class="controls-ipt" type="password" name="password" id="password" />
								</div>
							</div>
							<div class="control-group">
								<label class="control-label" for="email">
									<span class="warn">*</span> 新邮箱：</label>
								<div class="controls">
									<input type="text" class="controls-ipt" name="email" value="" id="email" />
								</div>
							</div>
							<div class="from-actions">
								<label class="control-label"></label>
								<input class="btn btn-primary"　data-loading-text="　……　" type="submit" value="　确定　">
								<input class="btn" type="reset" value="重置" />
							</div>
						</form>
					</div>
				</div>
				<div class="main-footer"></div>
			</div>
		</div>
		<!--{template side_bar}-->
	</div>
</div>

<!--{template web/footer}-->