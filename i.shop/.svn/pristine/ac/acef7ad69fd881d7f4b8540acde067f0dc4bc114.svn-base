.slect-uploadify{
  display: block;
  position: relative;
  width: 170px;
  height: 60px;
  overflow: hidden;
  border: 1px solid #EBEBEB;
  background: #F3F3F3 url("../img/upload.jpg") 0 0 no-repeat;
  cursor: pointer;
  text-align: center;
}
.slect-img{
  display: block;
  position: relative;
  width: 72px;slect-img
  height: 30px;
  overflow: hidden;
  border: 1px solid #EBEBEB;
  background: #F3F3F3;
  cursor: pointer;
  text-align: center;
}
.slect-img img{
  width: 60px;
  height: 60px;
}
.slect-img span {
  display: block;
  line-height: 30px;  
}
.slect-uploadify span{ 
  display: block;
  line-height: 60px;
}
.slect-img span {
  display: block;
  line-height: 30px; 
  color: #666;
  font-weight: bold; 
}
.filePrew {
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  height: 60px;
  line-height: 60px;
  font-size: 100px; /* 增大不同浏览器的可点击区域 */
  opacity: 0; /* 实现的关键点 */
  filter:alpha(opacity=0) \9;/* 兼容IE */
  *filter:alpha(opacity=0);/* 兼容IE7 */
  *margin-top: 4px;
}
.imgPrew{
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  height: 30px;
  line-height: 30px;
  font-size: 50px; /* 增大不同浏览器的可点击区域 */
  opacity: 0; /* 实现的关键点 */
  filter:alpha(opacity=0) \9;/* 兼容IE */
  *filter:alpha(opacity=0);/* 兼容IE7 */
  *margin-top: 4px;
}
.btn-file {
  position: relative;
  overflow: hidden;
  vertical-align: middle;
  display: inline-block;
  *display: inline; /* IE7 inline-block hack */
  *zoom: 1;
  cursor: pointer;
} 
.btn-file span {
  display: block;
}
.btn-file input {
  display: block;
  position: absolute;
  cursor: pointer;
  border-width: 0 0 100px 200px;
  background: transparent;
  border: 0;
  display: block;
  left: 0;
  top: 0;
  height: 30px;
  line-height: 30px;
  font-size: 100px; /* 增大不同浏览器的可点击区域 */
  opacity: 0; /* 实现的关键点 */
  filter:alpha(opacity=0);/* 兼容IE */
  *margin-top: 4px;
}
.fileupload {
  clear: both;
  display: inline-block;
  margin-bottom: 9px;
}

.fileupload .uneditable-input {
  display: inline-block;
  *display: inline; /* IE7 inline-block hack */
  *zoom: 1;
  margin-bottom: 0;
  vertical-align: middle;
  cursor: text;
}

.fileupload .thumbnail {
  display: inline-block;
  *display: inline; /* IE7 inline-block hack */
  *zoom: 1;
  margin-bottom: 5px;
  overflow: hidden;
  text-align: center;
  vertical-align: middle;
}

.fileupload .thumbnail > img {
  display: inline-block;
  max-height: 100%;
  vertical-align: middle;
}

.fileupload .btn {
  vertical-align: middle;
}
.fileupload-preview .btn {
  width: 58px;
  margin-top: 10px;
}
.fileupload-exists .fileupload-new,
.fileupload-new .fileupload-exists {
  font-size: 12px;
  text-align: center;
  line-height: 16px;
  cursor: pointer;
  width: 60px;
  position: absolute;
  bottom: 0px;
  left: 0px;
  right: 0px;
  padding: 5px 10px;
  background-color: rgba(0, 0, 0, 0.6);
  background-color: #000000 \9;
  filter: alpha(opacity=60);
  display: block;
}
.filupload-move {
  font-size: 12px;
  text-align: center;
  line-height: 16px;
  cursor: pointer;
  width: 80px;
  position: absolute;
  padding: 5px 10px;
  background-color: rgba(0, 0, 0, 0.6);
  background-color: #000000 \9;
  filter: alpha(opacity=60);
  display: block;
  margin-top: 70px;
}
.filupload-del {
  font-size: 12px;
  text-align: center;
  line-height: 16px;
  cursor: pointer;
  width: 70px;
  position: absolute;
  padding: 5px;
  background-color: rgba(0, 0, 0, 0.6);
  background-color: #000000 \9;
  filter: alpha(opacity=60);
  display: block;
  margin-top: -26px;
}
a.filupload-del,
a.fileupload-exists,
a.filupload-move {
  color: #C4CCD7;
}
a.filupload-del:hover,
a.fileupload-exists:hover,
a.filupload-move:hover {
  color: #fff;
}
.fileupload-add {
  color: #fff;
}
.fileupload .fileupload-preview {
  float:left;
  margin: 0 20px 0 0;
}
.fileupload .fileupload-info {
  margin-left: 110px;
}
.imgareaselect-border1 {
  background: url(/img/border-v.gif) repeat-y left top;
}

.imgareaselect-border2 {
    background: url(/img/border-h.gif) repeat-x left top;
}

.imgareaselect-border3 {
    background: url(/img/border-v.gif) repeat-y right top;
}

.imgareaselect-border4 {
    background: url(/img/border-h.gif) repeat-x left bottom;
}

.imgareaselect-border1, .imgareaselect-border2,
.imgareaselect-border3, .imgareaselect-border4 {
    filter: alpha(opacity=50);
  opacity: 0.5;
}

.imgareaselect-handle {
    background-color: #fff;
    border: solid 1px #000;
    filter: alpha(opacity=50);
    opacity: 0.5;
}

.imgareaselect-outer {
    background-color: #000;
    filter: alpha(opacity=50);
    opacity: 0.5;
}

