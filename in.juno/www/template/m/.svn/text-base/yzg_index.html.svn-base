			}
			.touchslider-item .touchslider-info {
				color: #fff;
			}
			.multi-line dd span {
				color: #666;
			}
			.multi-line dd a:hover {
				border-radius: 0;
				-webkit-border-radius: 0;
				-moz-border-radius: 0;
			}
			.multi-line .viewall a {
				color: #ef631e;
			}
			.multi-line .viewall a:hover {

			}
			.multi-line .address {
				font-size: 12px;
				color: #666;
				padding-top: 5px;
			}
			.touchslider-item img {
				height: 290px;
			}
			.touchslider-nav-item {

			}
			.feed, .user {
				border-bottom: 0;
