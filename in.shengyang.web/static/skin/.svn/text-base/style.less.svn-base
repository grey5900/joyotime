.placeico {
	background-image: url('ico.png');
	background-repeat: no-repeat;
	background-position: -191px 0;
	width: 11px;
	height: 13px;
	display: inline-block;
	margin-left: 5px;
	vertical-align: middle;
	overflow: hidden;
}
.placedico {
	background-image: url('ico.png');
	background-repeat: no-repeat;
	background-position: -125px -212px;
	width: 12px;
	height: 13px;
	display: inline-block;
	margin-right: 5px;
	vertical-align: middle;
	overflow: hidden;
}
.peopleico {
	background-image: url('ico.png');
	background-repeat: no-repeat;
	background-position: -125px -227px;
	width: 14px;
	height: 13px;
	display: inline-block;
	margin-right: 5px;
	vertical-align: middle;
	overflow: hidden;
}

