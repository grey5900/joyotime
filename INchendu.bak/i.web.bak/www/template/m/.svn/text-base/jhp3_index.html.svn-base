    <script type="text/javascript">
    // 抽獎
    var ito, speed, total, step, alert_message;
    var btn_start = $('#lottery-btn'),
        turnplate = $('.turnplate'),
        is_running = false, 
        max_loop = 5, // 循环总圈数
        i = 0, // 奖品默认开始索引
        all_awards = 8, // 奖品总数
        prizeId = 0,
        prizenum = 3;
        btn_start.on('click',function () {
          var arrPrize = {
            16:'5', //卡贴
            23:'0', //杯子
            17:'1', //钥匙链
            13:'4', //抱枕
            15:'7', //明信片      
            20:'3'//加多宝     
            };
          $.ajax({
          type:'POST',
          url:'/event/lottery/{$event_id}?'+new Date().getTime(),
          data:{uid:'{$uid}'},
          dataType:'jsonp',
          cache: false,
          success:function(json){
                  if(json.code == -1){ //未登陆
                    window.mobile && window.mobile.login ? window.mobile.login() : alert('请先登录IN成都！');
                  }else if(json.code == 0){//获奖了
                    prizeId = json.msg.prizeId;
                    prizenum = arrPrize[prizeId];
                    if (is_running) {
                      return false;
                    } else {// 初始化设置
                      step = 0; // 记录已跑的格数
                      speed = 80; // 初始速度
                      is_running = true;
                      if(total){
                        step = total - 50;
                      }
                      total = parseInt(prizenum) + max_loop * all_awards; // 总共格数加上一个随机数
                      turnplate.css('background', 'url(http://icd.sc/img/m/jh/wlot2.jpg) no-repeat');
                      turnplate.removeClass('retplate').addClass('runplate');
                      turnplate.find('i').removeClass();
                      $(".i"+i).find('i').addClass('run');
                      btn_start.addClass('disabled');
                      ito = setTimeout(rolling('恭喜您获得【' + json.msg.prizeName + '】。'), 100);
                    }
                  }else if(json.code == 1){//未获奖，还能抽奖
                    if (json.msg != 3) { //不满3次
                      prizenum = 6;
                    } else {  //满3次
                      prizenum = 2;
                    }
                    if (is_running) {
                      return false;
                    } else {
                      // 初始化设置
                      step = 0; // 记录已跑的格数
                      speed = 80; // 初始速度
                      is_running = true;
                      if(total){
                        step = total - 50;
                      }
                      total = parseInt(prizenum) + max_loop * all_awards; // 总共格数加上一个随机数
                      turnplate.css('background', 'url(http://icd.sc/img/m/jh/wlot2.jpg) no-repeat');
                      turnplate.removeClass('retplate').addClass('runplate');
                      turnplate.find('i').removeClass();
                      $(".i"+i).find('i').addClass('run');
                      btn_start.addClass('disabled');
                      ito = setTimeout(rolling('再接再厉'), 100);
                    }
                    }else if(json.code == 2){//不是会员
                      alert('不是会员');
                    }else if(json.code == 3){//奖品已发完，转后提示未中奖
                      if (is_running) {
                      return false;
                      } else {
                        // 初始化设置
                        step = 0; // 记录已跑的格数
                        speed = 80; // 初始速度
                        is_running = true;
                        if(total){
                          step = total - 50;
                        }
                        total = parseInt(prizenum) + max_loop * all_awards; // 总共格数加上一个随机数
                        turnplate.css('background', 'url(http://icd.sc/img/m/jh/wlot.jpg) no-repeat');
                        turnplate.removeClass('retplate').addClass('runplate');
                        turnplate.find('i').removeClass();
                        $(".i"+i).find('i').addClass('run');
                        btn_start.addClass('disabled');
                        ito = setTimeout(rolling('再接再厉'), 100);
                      }

                    }else if(json.code == 5){//已得过奖了
                      alert('您已经获得过奖品了,请把机会留给其他人吧');
                    }else if(json.code == 6){//抽奖已结束
                      alert('抽奖已结束');
                    }else if(json.code == 7){//未获奖，不能抽奖了
                      alert('每天只能抽3次哦，没关系，明天继续来挑战吧~');
                    }else if(json.code == 9){//没报名
                      alert('您还没有报名！请先报名');
                    }
                  }
               });
        });
    
    function rolling ( message ) {
      if(message) {
        alert_message = message
      }
      if ( step >= total ) {
        // 停止
        clearTimeout(ito);
        is_running = false;
        turnplate.removeClass('runplate').addClass('retplate');
        $(".i"+i).find('i').addClass('ret');
        btn_start.removeClass('disabled');
        alert(alert_message);
        return false;
      }
      i++;
      step++;
      if ( i >= 10 ) i = 0;
      if ( step < all_awards ) speed -= 5; // 第一圈加速
      if ( step >= (total - all_awards) ) speed += 15; // 最后一圈减速
      if ( speed <= 10 ) speed = 10; // 中间稳住
      
      turnplate.find('i').removeClass();
      $(".i"+i).find('i').addClass('run');
      ito = setTimeout(rolling, speed);
    }

        function alert( message ){
          var el = $("#alerter");
          //add dom
          if (!el[0]) {
              var el = $("<div id=\"alerter\"></div>");
              $("body").append(el);
          }
          el.html(message);
              var sTop = $(document).scrollTop(),
                sHeight = $(window).height(),
                sWidth = $(window).width(),
                elHeight = el.outerHeight(),
                elWidth = el.outerWidth(),
                elTop = (sHeight - elHeight) / 2 + sTop,
                elLeft = (sWidth - elWidth) / 2;
              el.css({
                  top: elTop + 10,
                  left: elLeft,
                  opacity: '0'
              })
              .animate({
                  opacity: '1',
                  top: elTop,
                  queue: true
              }, 200)
              .delay('3000')
              .animate({
                  opacity: '0',
                  top: elTop + 10,
                  queue: true
              }, 200,
              function() {
                el.remove();
              });
        }
