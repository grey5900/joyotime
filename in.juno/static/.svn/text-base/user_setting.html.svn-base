  			    <div class="fill">
  			      <form id="user-settings-form" class="form-horizontal" method="post" action="" >
  			        <div class="control-group">
                  <ul>
                    <li><label>用户名：</label><span>grey</span></li>
                    <li><label>积分：</label><span>1232</span></li>
                    <li><label>等级：</label><span>Lv34</span></li>
                  </ul>
  			        </div>
  			        <div class="control-group">
                  <label class="control-label"> 昵称： </label>
                  <div class="controls">
                    <input id="nickname" name="nickname" value="" maxlength="15" placeholder="" class="span10" type="text">
                  </div>
                </div>
                <div class="control-group">
                  <label class="control-label"> 签名： </label>
                  <div class="controls">
                    <textarea id="description" name="description" maxlength="30" rows="3" cols="40" type="text" ></textarea>
                  </div>
                </div>
                <div class="control-group">
                  <label class="control-label"> 性别： </label>
                  <div class="controls">
                    <label class="radio inline">
                      <input id="gender_male" type="radio" checked="checked" name="gender" value="">男
                    </label>
                    <label class="radio inline">
                      <input id="gender_female" type="radio" name="gender" value="">女
                    </label>
                  </div>
                </div>
                <div class="control-group">
                  <label class="control-label"> 生日： </label>
                  <div class="controls">
                    <select id="year" class="span3" name="birth_year"></select>
                    <select id="month" class="span3" name="birth_month"></select>
                    <select id="day" class="span3" name="birth_day"></select>
                    <script type="text/javascript">
                      $(function () {  
                          var year = $("#year");  
                          var month = $("#month");  
                          var day = $("#day");  
                          var date = new Date();  
                          var y = date.getFullYear().toString();  
                          MonDays = [31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31];  
                          //初始化年份  
                          year.get(0).options.add(new Option("年", "0"));  
                          for (var i = 1960; i <= y - 8; i++) {  
                              year.get(0).options.add(new Option(i, i));  
                          }  
                          year.attr("value", "{$profile[b_year]}"); //初始化默认值，1989应该是从数据库取出来的字段  
                          //初始化月份  
                          month.get(0).options.add(new Option("月", "0"));  
                          for (var i = 1; i < 13; i++) {  
                              month.get(0).options.add(new Option(i, i));  
                          }  
                          month.attr("value", "{$profile[b_month]}"); //初始化默认值，同上  
                          //初始化天数  
                          var mn = month.val();  
                          var ye = year.val();  
                          var n = MonDays[mn - 1];  
                          if ((ye % 4 == 0 && ye % 100 != 0) || ye % 400 == 0) {  
                              if (mn == 2) {  
                                  n++;  
                              }  
                          }  
                          day.get(0).options.add(new Option("日", "0"));  
                          for (var i = 1; i < n + 1; i++) {  
                              day.get(0).options.add(new Option(i, i));  
                          }  
                          day.attr("value", "{$profile[b_day]}"); //初始化默认值，同上
                          //月份改变时加载天数  
                          $("#month").change(function () {  
                              var mn = month.val();  
                              var ye = year.val();  
                              var n = MonDays[mn - 1];  
                              day.empty();  
                              if ((ye % 4 == 0 && ye % 100 != 0) || ye % 400 == 0) {  
                                  if (mn == 2) {  
                                      n++;  
                                  }  
                              }  
                              day.get(0).options.add(new Option("日", "0"));  
                              for (var i = 1; i < n + 1; i++) {  
                                  day.get(0).options.add(new Option(i, i));  
                              }  
                          });  
                      });  
                     </script>
