		<script type="text/javascript">
			$(function(){
				$('#header-search').on('submit',function(){
					var url     = $(this).attr('action'),
						keyword = $(this).find('#keyword');
					if (keyword.val() != "") {
						key = encodeURIComponent(keyword.val().replace("(","%").replace(")", ""));
						url += key;
						window.location.href = url;
					}else{
						keyword.focus();
					}
					return false;
				});
			});
		</script>
