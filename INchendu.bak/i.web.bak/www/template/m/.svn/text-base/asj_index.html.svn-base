	      5:'4', //七彩虹无线鼠标
	      6:'6', //招财猫
	      7:'0', //雨伞
	      8:'9', //鼠标垫
	      9:'1', //马克杯
	      10:'5' //IN成都T恤
	    };


	    $.ajax({
			type:'POST',
			url:'/event/lottery/{$event_id}?'+new Date().getTime(),
			data:{uid:'{$uid}'},
			dataType:'jsonp',
			cache: false,
			success:function(json){
              if(json.code == -1){
	            window.mobile && window.mobile.login ? window.mobile.login() : alert('请先登录IN成都！');
              }else if(json.code == 0){//获奖了
				prizeId = json.msg.prizeId;
				prizenum = arrPrize[prizeId];
			    if (is_running) {
			      return false;
			    } else {
			      // 初始化设置
			      step = 0; // 记录已跑的格数
			      speed = 80; // 初始速度
			      is_running = true;
			      if(total){
			      	step = total - 50;
			      }
			      total = parseInt(prizenum) + max_loop * all_awards; // 总共格数加上一个随机数
			      turnplate.css('background', 'url(/img/m2/wlot.gif) no-repeat');
			      turnplate.removeClass('retplate').addClass('runplate');
			      turnplate.find('i').removeClass();
			      $(".i"+i).find('i').addClass('run');
			      btn_start.addClass('disabled');
			      ito = setTimeout(rolling, 100);
			    }
				alert('恭喜您获得【' + json.msg.prizeName + '】。')
              }else if(json.code == 1){//未获奖，还能抽奖
              	  if (json.msg != 3) { //不满3次
					  prizenum = 7;
              	  } else {  //满3次
					  prizenum = 3;
              	  }
			      if (is_running) {
			        return false;
			      } else {
			        // 初始化设置
			        step = 0; // 记录已跑的格数
			        speed = 80; // 初始速度
			        is_running = true;
			        if(total){
			        	step = total - 50;
			        }
			        total = parseInt(prizenum) + max_loop * all_awards; // 总共格数加上一个随机数
			        turnplate.css('background', 'url(/img/m2/wlot.gif) no-repeat');
			        turnplate.removeClass('retplate').addClass('runplate');
			        turnplate.find('i').removeClass();
			        $(".i"+i).find('i').addClass('run');
			        btn_start.addClass('disabled');
			        ito = setTimeout(rolling, 100);
			      }
              }else if(json.code == 2){//未关注指定品牌
                alert('您还不是成都A世界电脑城的会员，请成为会员后再参与抽奖');
              }else if(json.code == 5){//已得过奖了
                alert('您已经获得过奖品了,请把机会留给其他人吧');
              }else if(json.code == 6){//抽奖已结束
                alert('抽奖已结束');
              }else if(json.code == 7){//未获奖，不能抽奖了
                alert('每天只能抽3次哦，没关系，明天继续来挑战吧~');
              }else if(json.code == 9){//没报名
            	alert('您还没有报名！请先报名');
              }

			
        	}
        });

});
    
    function rolling () {
      if ( step >= total ) {
        // 停止
        clearTimeout(ito);
        is_running = false;
        turnplate.removeClass('runplate').addClass('retplate');
        $(".i"+i).find('i').addClass('ret');
        btn_start.removeClass('disabled');
        return false;
      }
      i++;
      step++;
      if ( i >= 10 ) i = 0;
      if ( step < all_awards ) speed -= 5; // 第一圈加速
      if ( step >= (total - all_awards) ) speed += 15; // 最后一圈减速
      if ( speed <= 10 ) speed = 10; // 中间稳住
      
      turnplate.find('i').removeClass();
      $(".i"+i).find('i').addClass('run');
      ito = setTimeout(rolling, speed);
    }

        function alert( message ){
          var el = $("#alerter");
          //add dom
          if (!el[0]) {
              var el = $("<div id=\"alerter\"></div>");
              $("body").append(el);
          }
          el.html(message);
              var sTop = $(document).scrollTop(),
                sHeight = $(window).height(),
                sWidth = $(window).width(),
                elHeight = el.outerHeight(),
                elWidth = el.outerWidth(),
                elTop = (sHeight - elHeight) / 2 + sTop,
                elLeft = (sWidth - elWidth) / 2;
              el.css({
                  top: elTop + 10,
                  left: elLeft,
                  opacity: '0'
              })
              .animate({
                  opacity: '1',
                  top: elTop,
                  queue: true
              }, 200)
              .delay('3000')
              .animate({
                  opacity: '0',
                  top: elTop + 10,
                  queue: true
              }, 200,
              function() {
                el.remove();
              });
        }

    </script>

  </body>
