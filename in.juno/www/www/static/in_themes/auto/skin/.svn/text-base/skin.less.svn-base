@charset "utf-8";
@import "_variable.less";
@brown:#696053;
@bg:#b9ab9a;
@hover:#b9ab9a;
/**skin**/
a:hover{
	color:@hover;
}
.column{
	.hd{
		border-color:@brown;
		h2{
			background-color:@bg;
		}
	}
	.tit{
		h3{
			i{
				background-color:@brown;
			}
		}
	}
	& > .side .old_module .tit{
		border-color:@brown;
		h3{
			background-color:@bg;
		}
	}
}

.nav{
	ul{
		li{
			a{
				&:hover{
					color:@hover;
					i{
						background-image:url(images/nav-hover.png);
						_background-image:url(images/nav-hover-8.png);
					}
				}
				&.active{
					color:#696053;
					i{
						background-image:url(images/nav-active.png);
						_background-image:url(images/nav-active-8.png);
					}
				}
			}
			i{
				background:url(images/nav.png) no-repeat;
				_background-image:url(images/nav-8.png);
				&.zy,&.cat26{
					background-position:-125px 0;
				}
				&.yh,&.cat27{
					background-position:-250px 0;
				}
				&.ws,&.cat130{
					background-position:-375px 0;
				}
				&.dql,&.cat29{
					background-position:-500px 0;
				}
				&.ht,&.cat30{
					background-position:-625px 0;
				}
			}
		}
	}
}
/**end**/
.channel .channel_nav{
	.con{
		li{
			&:hover{
				background:@brown;
				a{
					color:#fff;
					i{
						background-image:url(images/topic-white.png);
						_background-image:url(images/topic-white-8.png);
					}
				}
			}
			i{
				background:url(images/topic.png) no-repeat;
				_background-image:url(images/topic-8.png);
				&.zy,&.cat26{
					background-position:0 -45px;
				}
				&.yh,&.cat27{
					background-position:0 -89px;
				}
				&.cyh,&.cat28{
					background-position:0 -133px;
				}
				&.ws,&.cat29{
					background-position:0 -177px;
				}
				&.dql,&.cat30{
					background-position:0 -221px;
				}
			}
		}
		.ft{
			background:url(images/tel.jpg) no-repeat;
		}
	}
}





a{
	color:@brown;
}
.topic_nav{
	
	li{
		
		a{
			
			&.active{
				border-color:@brown;
				
			}
			&:hover{
				color:@hover;
			}
		}
	}
}

.ssss{
	.con{
		li{
			margin-bottom:10px;
		}
		.img{
			img{
				width:254px;
				height:142px;
				border:3px solid #eee;
			}
		}
		.info{
			padding:5px 10px 0;
			a{
				display:block;
				font:14px/1.5 @yh;
			}
			p{
				color:#999;
			}
		}
	}
}
.channel .friends{
	li{
		a:hover{
			color:@brown
		}
	}
}

.auto_list/*,.channel_list*/{
