        background: #fff;
      }
      /* font color  */
      .unit-title {
        color: #5ba81c;
      }
      #content .unit p {
        color: #666;
      }
      #content .statement p {
        color: #666;
      }
      .box-db-left, .box-db-right {
        color: #5a146a;
      }
      .texts .info-group {
        color: #0c425c;
        border-bottom: 1px dotted #e1d5c5;
      }
      .texts .inside strong {
        color: #5ba81c;
      }
      .texts .inside p {

      }
      .bottons .botton span.title {
      }
      .bottons .botton span.intro {

      }
      .touchslider-item .touchslider-info {
