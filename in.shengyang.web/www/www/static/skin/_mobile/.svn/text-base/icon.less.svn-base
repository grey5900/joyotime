// icons
//---------
.undone {
  display:inline-block;
  padding: 1px;
  margin: 0;
  min-height: 12px;
  font-size: 10px;
  text-align: center;
  vertical-align: middle;
  cursor: pointer;
  .border-radius(50px);
  width: 40px;
  .doneColor();
}
.done {
  background-image: url(/static/m_img/done.png);
  background-position: 100% 50%;
  background-repeat: no-repeat;
  display: inline-block;
  width: 15px;
  height: 15px;
}
.arrow {
  background-image: url('/static/m_img/arrow.png');
  background-position: 100% 50%;
  background-repeat: no-repeat;
  .background-size(7px 12px);
  // &:hover{
    // background-image: url('/static/m_img/m_arrow_hover.png');
    // background-position: 100% 50%;
    // background-repeat: no-repeat;
  // }
}
.m_earn {
  background-image: url('/static/m_img/m_earn.png');
  background-repeat: no-repeat;
  .background-size(56px 20px);
  width: 56px;
  height: 20px;
  display: inline-block;
}
.icon-arrow {
  background-image: url('/static/m_img/arrow.png');
  background-position: 100% 50%;
  background-repeat: no-repeat;
}
.dotted {
  background: url('/static/m_img/dotted.jpg') repeat-x scroll 0 bottom transparent;
}
.doneColor {
  background-color: #f04747;
  color: #fff;
}
.boxTitleNormal {
  color: @font-default;
}
.blockTitle {
  margin-left: 8px;
}
