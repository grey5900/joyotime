.imgs{
  *zoom: 1;
  display: inline-block;
  *display: inline; /* IE7 inline-block hack */
  *zoom: 1;
  clear: both;
  margin:0;
  padding:0;
  height: auto;
}
.imgs li{
  margin-right:10px;
  padding:1px;
  float:left;
  margin-bottom: 10px;
}
.up-pic {
  float: left;
  margin-bottom: 20px;
  margin-right: 10px;
  width: 360px;
  border: solid 2px #eef1e6;
  background: #fff;
  padding: 1em;
}
.up-pic #div_upload_big {
  width: 360px;
}
.up-pic #div_upload_big img {
  max-width: none;
}
.show-pic {
  float: left;
  width: 190px;
  border: solid 2px #eef1e6;
  background: #ffffff;
  padding: 1em;
}
.show-pic #preview {
  width: 190px;
  overflow: hidden;
}
.show-pic #preview img {
  width: 190px;
  max-width: none;
}
#upload_thumb {
  display: block;
}
.uploads-img {
  width:360px;
}
.uploads-img > ul {
  margin: 0;
}

