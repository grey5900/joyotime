.thumbnails {
  list-style: none;
  margin: 15px 0 0 -12px;
  padding-bottom: 40px;
  *zoom: 1;
}
.thumbnails:before,
.thumbnails:after {
  display: block;
  content: "";
  line-height: 0;
}
.thumbnails:after {
  clear: both;
}
a.thumbnail:hover {
  border-color: #333333;
  -webkit-box-shadow: 0 1px 4px rgba(0, 105, 214, 0.25);
  -moz-box-shadow: 0 1px 4px rgba(0, 105, 214, 0.25);
  -ms-box-shadow: 0 1px 4px rgba(0, 105, 214, 0.25);
  box-shadow: 0 1px 4px rgba(0, 105, 214, 0.25);
}
.thumbnail > img {
  display: block;
  max-width: 100%;
  margin-left: auto;
  margin-right: auto;
}
.thumbnail .caption {
  padding: 9px;
  color: #555555;
}
.thumbnails > li {
  float: left;
}
.thumbnail {
  float: left;
  margin: 0 0 12px 12px;
  width: 238px;
  display: block;
  border: 1px solid #e6e8e8 \9;
  background: #ffffff;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  -ms-border-radius: 3px;
  border-radius: 3px;
  -webkit-box-shadow: 0 1px 1px rgba(62, 68, 68, 0.25);
  -moz-box-shadow: 0 1px 1px rgba(62, 68, 68, 0.25);
  -ms-box-shadow: 0 1px 1px rgba(62, 68, 68, 0.25);
  box-shadow: 0 1px 1px rgba(62, 68, 68, 0.25);
}
.thumbnail:hover {
  -webkit-box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.2);
  -moz-box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.2);
  -ms-box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.2);
  box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.2);
}
.thumbnail .photo {
  position: relative;
  width: 238px;
  border-bottom: 1px solid #f3f3f3;
  background: #fff;
  *zoom: 1;
}
.thumbnail .photo:before,
.thumbnail .photo:after {
  display: block;
  content: "";
  line-height: 0;
}
.thumbnail .photo:after {
  clear: both;
}
.thumbnail .photo img {
  width: 220px;
  display: block;
  margin: 9px;
}
.thumbnail .photo p {
  font-size: 12px;
  float: 20px;
  color: #666;
  width: 220px;
  margin: 9px;
}
.thumbnail .user {
  padding: 9px;
  background: #fafafa;
  *zoom: 1;
}
.thumbnail .user:before,
.thumbnail .user:after {
  display: block;
  content: "";
  line-height: 0;
}
.thumbnail .user:after {
  clear: both;
}
.thumbnail .user .avatar {
  float: left;
  width: 30px;
  height: 30px;
  margin-right: 10px;
  display: block;
}
.thumbnail .user .avatar img {
  width: 30px;
  height: 30px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  border-radius: 4px;
  display: block;
}
.thumbnail .user .detail {
  margin: 0 60px 0 30px;
  line-height: 14px;
}
.thumbnail .user .detail .username {
  color: #d52f2f;
  font-weight: bold;
  display: block;
}
.thumbnail .user .detail .data {
  color: #aaa;
  display: inline-block;
  *display: inline;
  *zoom: 1;
}
.thumbnail .user .praise,
.thumbnail .user .praised {
  margin-top: 12px;
  float: right;
}
.thumbnail .user .detail-place {
  display: inline-block;
  *display: inline;
  *zoom: 1;
}
.thumbnail .user .detail-place .username {
  color: #d52f2f;
  font-weight: bold;
  display: block;
}
.thumbnail .user .detail-place .data {
  color: #aaa;
  display: inline;
}
.media,
.media-body {
  overflow: hidden;
  zoom: 1;
}
.media,
.media .media {
  margin-top: 15px;
}
.media:first-child {
  margin-top: 0;
}
.media-object {
  display: block;
}
.media-heading {
  margin: 0 0 5px;
}
.media .pull-left {
  margin-right: 10px;
}
.media .pull-right {
  margin-left: 10px;
}
.media-list {
  margin-left: 0;
  list-style: none;
}
.label,
.badge {
  display: inline-block;
  padding: 2px 4px;
  font-size: 10.152px;
  font-weight: bold;
  line-height: 14px;
  color: #fff;
  vertical-align: baseline;
  white-space: nowrap;
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
  background-color: #999999;
}
.label {
  border-radius: 3px;
}
.badge {
  padding-left: 9px;
  padding-right: 9px;
  border-radius: 9px;
}
.label:empty,
.badge:empty {
  display: none;
}
a.label:hover,
a.badge:hover {
  color: #fff;
  text-decoration: none;
  cursor: pointer;
}
.label-danger,
.badge-danger {
  background-color: #b94a48;
}
.label-danger[href],
.badge-danger[href] {
  background-color: #953b39;
}
.label-warning,
.badge-warning {
  background-color: #f89406;
}
.label-warning[href],
.badge-warning[href] {
  background-color: #c67605;
}
.label-success,
.badge-success {
  background-color: #468847;
}
.label-success[href],
.badge-success[href] {
  background-color: #356635;
}
.label-info,
.badge-info {
  background-color: #3a87ad;
}
.label-info[href],
.badge-info[href] {
  background-color: #2d6987;
}
.label-inverse,
.badge-inverse {
  background-color: #333333;
}
.label-inverse[href],
.badge-inverse[href] {
  background-color: #1a1a1a;
}
.btn .label,
.btn .badge {
  position: relative;
  top: -1px;
}
.btn-mini .label,
.btn-mini .badge {
  top: 0;
}
