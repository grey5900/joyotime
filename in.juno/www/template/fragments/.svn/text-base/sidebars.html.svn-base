{if $row['frag']['style']=="photo_list"}
<!-- photo_list -->
<div class="module old_module">
	<div class="tit"><h3>{$row['frag']['name']}</h3>{if $row['frag']['rule']['jump_link']}<a class="more" href="{$row['frag']['rule']['jump_link']}" >更多</a>{/if}</div>
	<div class="con">
		<ul class="list-photo">
		{loop $row['data'] $value}
			<li><a href="{$value['link']}" target="_blank" class="thumb"  title="{$value['title']}"><img src="{$value['image']}" alt="{$value['title']}"  /></a><a href="{$value['link']}" target="_blank" class="title" title="{$value['title']}">{$value['title']}</a>{if $value['summary']}<p class="summary">{eval echo cut_string($value['summary'],48)}</p>{/if}</li>
		{/loop}
		</ul>
	</div>
</div>
<!-- photo_list -->
{/if}

