    <script type="text/javascript">
			$(function() {
	    		$("#join-btn").on("click", function(){
	    			$.ajax({
	    				type:'POST',
	    				url:'/event/join_v2',
	    				data:{event_id:'{$event_id}',uid:'{$uid}',lat:'{$lat}', lng:'{$lng}'},
	    				dataType:'jsonp',
	    				cache: false,
	    				success:function(json){
	    					if(json.code == -1) {
					            window.mobile && window.mobile.login ? window.mobile.login() : alert('请先登录IN成都！');
	    					}else if(json.code == 0){//报名成功
	    						alert('报名成功，下一步');
	    						var num = new Number($("#joins_num").html());
	    						num = num + 1;
	    						$("#joins_num").html(num);
	    					}else if(json.code == 1){
	    					  alert('真抱歉！报名的人太多了，请稍后再试试');
	    					}else if(json.code == 2){
	    					  alert('您已经报名参与了这个活动');
	    					}else if(json.code == 3){
	    					  alert('真抱歉！这个活动已停止报名了');
	    					}else if(json.code == 4){
	    					  alert('对不起！请您先登录');
	    					}else{//系统错误了
	    					  alert('真抱歉！这个活动已停止报名了');
	    					}
	    				}
	    			});
	    		});

				<!--{if $cfg['gen_prize_list']}-->
				//生成奖品清单
	             $.get('/event/put_prize/{$event_id}?'+new Date().getTime(), function(html){});
				<!--{/if}-->
				$(".expand").each(function() {
					$(this).on("click", function() {
						$(this).toggleClass("on");
					});
				});
				$(".touchslider").touchSlider({
					mouseTouch : true,
				});
				var settime;
				$(".scrolling").hover(function() {
					clearInterval(settime);
				}, function() {
					settime = setInterval(function() {
						//wtf
						var fst = $(".scrolling ul:first");
						var height = fst.find("li:first").outerHeight();
						fst.animate({
							"marginTop" : -height + "px"
						}, 600, function() {
							fst.css({
								marginTop : 0
							}).find("li:first").appendTo(fst);
						});
					}, 3000);
				}).trigger("mouseleave");

			})
    </script>
