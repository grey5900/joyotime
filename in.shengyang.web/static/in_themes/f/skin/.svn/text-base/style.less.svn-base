@import "../../../skin/bootstrap/variables.less";
@import "../../../skin/bootstrap/mixins.less";

/* 频道主色调 */
@channel-fore: #313130;
@channel-back: #8d2426;
@channel-back-image: 'channel-style.png';

body {
}

//全屏图片链接
a.fullscreen {
	display: block;
	width: 1000px;
	height: 140px;
	margin: 0 auto;
}

#container {
  background: url('fullscreen.jpg') center 0 no-repeat;
  .container {
    background: url(@channel-back-image) -230px top repeat-y;
  }
}
#container-page {
  .container {
    background: url(@channel-back-image) -230px top repeat-y;
  }
}
.main-l {
  border-top: 2px solid @channel-back;
}
/* nav*/
.nav-bg {
	background: url(@channel-back-image) no-repeat;
}
.nav {
	float: left;
	width: 190px;
	.tit {
		height: 71px;
		margin: 0px;
		padding: 0px;
		text-indent: -9999px;
	}
	.col {
		.cleartype();
		a {
			height: 26px;
			line-height: 26px;
			display: block;
			background-color: @channel-back;
			background-position: 0 -116px;
			padding: 9px 0 9px 19px;
			font-size: 16px;
			color: #ffffff;
			.ico {
				background: url('nav-ico.png') no-repeat;
				width: 26px;
				height: 26px;
				float: left;
				display: block;
				margin-right: 9px;
				&.qss { background-position: 0 -27px; }
				&.mao { background-position: 0 -54px; }
				&.fang { background-position: 0 -81px; }
				&.che { background-position: 0 -108px; }
				&.ssss { background-position: 0 -135px; }
				&.xin { background-position: 0 -162px; }
			}
			&:hover,
			&.on {
				text-decoration: none;
				color: #fff;
				background-color: @channel-fore;
				background-position: 0 -71px;
			}
		}
	}
	.tag {
		padding: 20px 11px 0;
		a {
			background: lighten(@channel-back,8);
			height: 30px;
			line-height: 30px;
			padding: 0 10px;
			display: inline-block;
			margin-bottom: 10px;
			color: lighten(@channel-back,50);
			text-decoration: none;
			&:hover {
				background-color: lighten(@channel-back,16);
			}
		}
	}
	.service {
		width: 190px;
		height: 50px;
		text-indent: -9999px;
		overflow: hidden;
		margin-top: 10px;
		background: url(@channel-back-image) 0 -250px no-repeat;
	}
}
.main {
	float: right;
	width: 810px;
}
.main-l {
	float: left;
	width: 560px;
	border-top: 2px solid @channel-back;
}
.main-r {
	float: right;
	width: 230px;
}

/* 热点*/
.hot {
	background: #fff;
	padding: 14px;
	border: 1px #eaeaea solid;
	border-top: 2px solid @channel-back;
	margin-bottom: 20px;
	.clearfix();
	.headline {
		width: 365px;
		padding-left: 15px;
		float: right;
		.list {
			list-style: none;
			margin: 0px;
			padding: 0px;
			li {
				line-height: 20px;
				border-bottom: 1px #ccc dashed;
				padding-bottom: 6px;
				margin-bottom: 6px;
				h1 {
					margin: 0px;
					padding: 0px;
					font-size: 18px;
					text-align: center;
					line-height: 24px;
					font-weight: normal;
					.cleartype();
				}
				p {
					margin: 0px;
					padding: 0px;
					color: #666666;
					font-size: 12px;
					text-align: center;
					font-weight: normal;
				}
				&.more {
					border: none;
					padding-bottom: 0px;
					margin-bottom: 0px;
					line-height: 23px;
					a {
						display: inline-block;
					}
				}
			}
		}
	}
	hr {
		clear: both;
		visibility: hidden;
		margin: 0px;
	}

	.search {
		margin-top: 10px;
		padding-top: 10px;
		border-top: 3px solid #eeeeee;
		.tit {
			ul {
				margin: 0px;
				padding: 0px;
				*zoom: 1;
				.clearfix();
				li {
					float: left;
					font-size: 14px;
					font-weight: bold;
					line-height: 20px;
					padding: 0 5px 9px;
					a {
						text-decoration: none;
					}
					&.active {
						background: url('../../../skin/arrow-top.png') center bottom no-repeat;
						a {
							color: @channel-back;
						}
					}
					&.pipe {
						width: 10px;
						text-align: center;
						font-weight: normal;
						color: #cccccc;
					}
				}
			}
		}
		.con {
			clear: both;
			padding: 9px;
			background: #f2f2f2;
			form {
				margin: 0px;
				.input-box {
					.btn {
						font-size: 14px;
					}
					#keywords {
						width: 580px;
					}
				}
				.more {
					margin-top: 10px;
					select {
						width: 100px;
						border-radius: 0;
					}
				}
			}
		}
	}

}

/*
 * 团房
 */
.groupon {
	border-right: 1px #e6e6e6 solid;
	border-bottom: 1px #e6e6e6 solid;
	background: #fff;
	margin-bottom: 20px;
	.tit {
		height: 33px;
		line-height: 33px;
		background: #eee;
		padding-left: 18px;
		zoom: 1;
		h2 {
			padding: 0px;
			margin: 0 10px 0 0;
			font-weight: normal;
			font-size: 18px;
			line-height: 33px;
			float: left;
			.cleartype();
		}
	}
	.con {
		padding: 15px;
		.clearfix();
		.thumb {
			float: left;
			img	{
				width: 370px;
				height: 260px;
			}
		}
		.detail {
			float: right;
			width: 145px;
			height: 260px;
			position: relative;
			color: #666666;
			h3 {
				font-size: 14px;
				margin: 0px;
				padding: 0px;
				line-height: 30px;
			}
			.info {
			}
			.count {
				padding: 5px 5px 10px;
				color: #ffffff;
				line-height: 20px;
				background: url(@channel-back-image) 0px -320px no-repeat;
			}
			.prefer {
				margin-top: 5px;
			}
			.btn {
				position: absolute;
				left: 0px;
				right: 0px;
				bottom: 0px;
			}
		}
	}
}

/* 焦点幻灯*/
#hd1 {
	width: 400px;
	height: 300px;
	float: left;
}
.focus {
	position: relative;
	overflow: hidden;
	font-size: 0;
}
#hd1 .focus img,#hd1 .focus {
	width: 400px;
	height: 300px;
}
.focus_Num,.focus .bg-al {
	position: absolute;
	bottom: 0px;
	left: 0px;
	overflow: hidden;
	width: 400px;
	height: 50px;
}
.focus_Num ul {
	list-style: none;
	margin: 0px;
	padding: 0px;
}
#hd1 .focus_Num ul,#hd1 .focus_Num li {
	line-height: 30px;
	line-height: 30px;
}
.focus_Num ul {
	overflow: hidden;
}
.focus_Num li {
	display: block;
	display: none;
	font-size: 14px;
	color: #eee;
	text-align: center;
}
.focus_Num span {
	cursor: pointer;
	display: inline-block;
}
.focus_Numid {
	text-align: center;
	padding-top: 9px;
}
#hd1 .focus_Num span {
	width: 17px;
	height: 3px;
	background: #727272;
	overflow: hidden;
	margin-right: 3px;
}
#hd1 .focus_Num span.hov {
	background: #c82e32;
}
.focus .bg-al {
	filter: alpha(opacity=60);
	-moz-opacity: 0.6;
	opacity: 0.6;
	background: #000;
	height: 50px;
}

.push {
	border-right: 1px #e6e6e6 solid;
	border-bottom: 1px #e6e6e6 solid;
	background: #fff;
	.tit {
		height: 33px;
		line-height: 33px;
		background: #eee;
		padding-left: 18px;
		zoom: 1;
		h2 {
			padding: 0px;
			margin: 0 10px 0 0;
			font-weight: normal;
			font-size: 18px;
			line-height: 33px;
			float: left;
			.cleartype();
		}
		.say {
			width: 68px;
			height: 25px;
			margin: 4px 0 0 0;
			background: url(@channel-back-image) 0 -160px no-repeat;
			display: block;
			float: left;
			color: #fff;
			font-weight: bold;
			font-size: 14px;
			text-align: center;
			text-decoration: none;
			line-height: 28px;
			padding-left: 10px;
		}
		.tab {
			float: right;
			width: 180px;
			height: 40px;
			a {
				display: block;
				width: 90px;
				height: 40px;
				line-height: 30px;
				text-align: center;
				color: #333;
				font-size: 18px;
				float: left;
				text-decoration: none;
				.cleartype();
				&.current {
					color: #fff;
					background-position: -140px -160px;
				}
			}
		}
	}
	.con {
		padding-bottom: 45px;
	}
}

/* 重设频道头部样式 */
#header {
	border-bottom-color: @channel-back;
	.site-nav {
		li {
			a {
				&.on {
					border-bottom-color: @channel-back;
				}
			}
		}
	}
}

/* 重设频道模块样式 */
.module {
	.tit {
		h3 {
			background: url(@channel-back-image) 0px -200px no-repeat;
		}
	}
	.con {}
	.list-groupon {
		.price {
			.p { color: @channel-back;}
		}
	}
	&#weibo {
		.tit {
			padding: 0 0 10px;
			ul {
				margin: 0px;
				padding: 0px;
				li {
					float: left;
					font-size: 18px;
					line-height: 26px;
					a {
						text-decoration: none;
					}
					&.active {
						border-bottom: 2px solid @channel-back;
					}
					&.pipe {
						width: 10px;
						text-align: center;
						color: #cccccc;
					}
				}
			}
		}
		.con {
			.tab-pane {
				display: none;
				&.active {
					display: block;
				}
			}
		}
	}

}
