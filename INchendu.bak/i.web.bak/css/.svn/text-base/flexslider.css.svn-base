.hide-text{font:0/0 a;color:transparent;text-shadow:none;background-color:transparent;border:0}.input-block-level{display:block;width:100%;min-height:28px;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;-ms-box-sizing:border-box;box-sizing:border-box}.no-js .slide-list>li:first-child{display:block}.a1-slide{background:#fff;position:relative;-webkit-border-radius:5px;-moz-border-radius:5px;-o-border-radius:5px;border-radius:5px;zoom:1}.a1-slide .slide-list{zoom:1}.a1-slide .slide-list li{float:left;position:relative}.a1-slide .slide-pagination{width:100%;height:30px;position:absolute;bottom:0;text-align:center;background:none repeat scroll 0 0 #0d0900;opacity:0.7;filter:alpha(opacity=70)}.a1-slide .slide-pagination li{margin:0 0 0 5px;display:inline-block;zoom:1;*display:inline;margin-top:10px}.a1-slide .slide-pagination li a{width:8px;height:8px;display:block;background:url(/img/bg_control_nav.png) no-repeat 0 0;cursor:pointer;text-indent:-9999px}.a1-slide .slide-pagination li a:hover{background-position:0 0}.a1-slide .slide-pagination li:first-child{margin:0}.a1-slide .slide-pagination li.active a{background-position:0 -9px;cursor:default}