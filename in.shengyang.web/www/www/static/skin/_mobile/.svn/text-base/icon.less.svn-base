// wap icons
.wap-icons() {
  background:transparent url('/static/m_img/wap_ions.png') repeat-x scroll 0 -999em;
  display: inline-block;
  -webkit-background-size: 50px 18px;
  background-size: 50px 18px;
  margin-right: 10px;
}
.wap-icons-tel {
  .wap-icons();
  width: 13px;
  height: 18px;
  background-position:0 0;
}
.wap-icons-meil {
  .wap-icons();
  width: 18px;
  height: 18px;
  background-position:-66px 0;
}
.wap-icons-address {
  .wap-icons();
  width: 16px;
  height: 18px;
  background-position:-34px 0;
}

