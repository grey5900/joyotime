.section-layout {
  *zoom: 1;
}
.section-layout:before,
.section-layout:after {
  display: table;
  content: "";
}
.section-layout:after {
  clear: both;
}
.content {
  float: left;
  width: 900px;
  padding: 0;
  margin: 0;
  background: url(/img/timeline_line.png) center 0px repeat-y;
}
.content .header-info-box {
  border: 1px solid #e6e8e8 \9;
  background: white;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  -ms-border-radius: 3px;
  border-radius: 3px;
  -webkit-box-shadow: 0 1px 1px rgba(62, 68, 68, 0.25);
  -moz-box-shadow: 0 1px 1px rgba(62, 68, 68, 0.25);
  box-shadow: 0 1px 1px rgba(62, 68, 68, 0.25);
  padding: 15px;
}
.aside {
  float: right;
  width: 80px;
  padding: 0;
  margin: 0;
  top: 85px;
}
.aside .fixed {
  position: fixed;
  top: 0;
}
.aside .timeline-timebar {
  width: 73px;
  padding: 7px 0 0 7px;
}
.aside .timeline-timebar .timebar-side span,
.aside .timeline-timebar .timebar-oldside span {
  display: block;
  color: #999999;
  font-weight: bold;
  line-height: 22px;
  margin-bottom: 1px;
}
.aside .timeline-timebar .timebar-side a,
.aside .timeline-timebar .timebar-oldside a {
  display: block;
  line-height: 22px;
  padding: 0 0 0 5px;
  border-left: 5px solid #5caa1d;
  margin-bottom: 1px;
}
.aside .timeline-timebar .timebar-side a:hover,
.aside .timeline-timebar .timebar-oldside a:hover {
  border-left: 5px solid #447e16;
}
.aside .timeline-timebar .timebar-side .nav,
.aside .timeline-timebar .timebar-oldside .nav {
  border: none;
  background-color: transparent;
  margin: 0;
}
.aside .timeline-timebar .timebar-side .active a,
.aside .timeline-timebar .timebar-oldside .active a {
  color: #447e16;
  border-left: 5px solid #447e16;
}
.timeline-box {
  padding-top: 25px;
}
.timeline-box .timeline-section {
  *zoom: 1;
}
.timeline-box .timeline-section:before,
.timeline-box .timeline-section:after {
  display: table;
  content: "";
}
.timeline-box .timeline-section:after {
  clear: both;
}
.timeline-box .timeline-hd-title {
  cursor: default;
  display: block;
  color: white;
  text-decoration: none;
  margin-top: 10px;
  opacity: 0;
  filter: alpha(opacity=0) \9;
}
.timeline-box .timeline-hd-title span {
  display: block;
  font-weight: bold;
  line-height: 30px;
  margin: 0px auto 15px;
  text-align: center;
  width: 100px;
  height: 30px;
  background: #87c347;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  -ms-border-radius: 3px;
  border-radius: 3px;
  *zoom: 1;
}
.timeline-box .timeline-hd-title span:before,
.timeline-box .timeline-hd-title span:after {
  display: table;
  content: "";
}
.timeline-box .timeline-hd-title span:after {
  clear: both;
}
.timeline-box .timeline-hd-title.loading {
  opacity: 1;
  filter: alpha(opacity=100) \9;
  background: url(/img/loading.gif) center bottom no-repeat;
  padding-bottom: 32px;
}
.timeline-box .timeline-hd-title.loaded {
  opacity: 1;
  filter: alpha(opacity=100) \9;
}
.timeline-box .timeline-hd-title.now span {
  display: none;
}
.timeline-box .timeline-adventitia {
