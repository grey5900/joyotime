.car_discount{
	.zysc{
		.con{
			height:298px;
			*height:294px;
			_height:304px;
		}
	}
}
.zysc{
	.con{
		padding:15px 10px 12px;		
		.img{
			position:relative;
			height:144px;
			.caption{
				position:absolute;
				background:#000;
				width:100%;
				height:20px;
				line-height:20px;
				color:#fff;
				text-align:center;
				filter:alpha(opacity=60);
				
				opacity:.6;
				bottom:0;
				left:0;
				z-index:100;
			}
			img{
				width:260px;
				height:144px;
			}
		}
		.info{
			h3{
				text-align:center;
				font:bold 14px/2 @yh;
				a{
					color:#bb0000;
				}
			}
			p{
				text-indent:2em;
				color:#999;
				line-height:22px;
				a{
					color:#666;
				}
			}
		}
	}
}
.yhhq{
	.con{
		.img{
			img{
				border:none;
				width:260px;
				padding:0;
				height:130px;
			}
		}
	}
}
.wsss{
	.bd{
		.list{
			*zoom:1;
			margin-left:-30px;
			
		}
		.list li{
			_display:inline;
			margin-left:30px;
			width:298px;
			height:292px;
			border:@bd-grey;
			overflow:hidden;
			float:left;
			margin-bottom:40px;
			.img{
				img{
					height:169px;
					width:100%;
				}
			}
			a{
				display:block;
				height:100%;
				text-decoration:none;
				&:hover{
					background:#b9b9b9;
					.info{
						& h4,h5,p{
							color:#fff;
						}
						& h4,h5{
							font-weight:bold;
						}
					}
				}
			}
			.info{
				padding:10px;
				
				margin-top:10px;;
				& h4,h5,p{
					line-height:24px;
				}
				h4{
					font-size:18px;
					color:@brown;
					
				}
				h5{
					font-size:16px;
					color:#988977;
					
				}
				p{
					font-size:14px;
					color:#999;
				}
			}
		}
	}
}