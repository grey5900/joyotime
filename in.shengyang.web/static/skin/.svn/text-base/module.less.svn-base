.module {
	clear: both;
	margin-bottom: 12px;
	.tit {
		background: url('ico.png') bottom repeat-x;
		padding: 0 0 16px;
		.cleartype();
		.clearfix();
		h3 {
			color: #333;
			font-size: 18px;
			font-weight: normal;
			line-height: 20px;
			margin: 0px;
			padding: 0 0 0 17px;
			float: left;
		}
		.more {
			float: right;
			font-size: 12px;
			line-height: 20px;
			padding-right: 10px;
			background: url('ico.png') no-repeat;
			background-position: right -49px;
		}
	}
	.tit-first {
		background: url('ico.png') bottom repeat-x;
		height: 4px;
		.clearfix();
	}
	.con {
		background-position: 0 -296px;
		background-repeat: repeat-x;
		border: 1px #e6e6e6 solid;
		border-top: 0 none;
		padding: 9px 9px 0;
		background-color: #fbfbfb;
		.box-shadow(0px 1px 0px rgba(0,0,0,0.03));
	}
	.list-first {
		.place-card {
			img {
				float: left;
				width: 60px;
				height:60px;
				margin-right: 10px;
			}
			.info {
				margin-left: 70px; 
				h3 {
					font-size: 16px;
					line-height: 30px;
					margin: 0;
				}
				p {

					color: #666;
					line-height: 18px;
				}
				.star-info {
					margin-bottom: 5px;
				}
			}
			.details {
				border-top: 1px dotted #d6d6d6;
				border-bottom: 1px dotted #d6d6d6;
				padding: 10px 0;
				margin: 10px 0 20px;
				color: #666;
				display: inline-block;
				.bor {
					border-left: 1px solid #d6d6d6;
    				border-right: 1px solid #d6d6d6;
				}
				li {
					float: left;
					width: 86px;
					text-align: center;
				}
				a {
					font-size: 26px;
					.cleartype();
				}
				p {
					font-size: 14px;
					font-weight: bold;
				}
			}
		}
		.link-sj {
			text-align: center;
			margin-bottom: 20px;
			a {
				width: 160px;
				font-size: 16px;
			}
		}
	}
	.list-like {
		list-style: none;
		margin: 0px;
		padding: 0px;
		li {
			height: 90px;
			margin-bottom: 10px;
			position: relative;
			img {
				width: 260px;
				height: 90px;
			}
			b {
				display: block;
				width: 210px;
				height: 25px;
				background: #000;
				position: absolute;
				left: 0;
				bottom: 0;
				filter: alpha(opacity=70);
				-moz-opacity: 0.7;
				opacity: 0.7;
			}
			.t {
				display: block;
				width: 210px;
				height: 25px;
				line-height: 25px;
				position: absolute;
				left: 0;
				bottom: 0;
				color: #fff;
				font-weight: bold;
				padding: 0 8px;
				width: 194px;
				span {
					color: #aaa;
					float: right;
				}
			}
		}
	}
	.list-event {
		list-style: none;
		margin: 0px;
		padding: 0px;
		li {
			height: 90px;
			margin-bottom: 10px;
			position: relative;
			img {
				width: 210px;
				height: 90px;
			}
			b {
				display: block;
				width: 210px;
				height: 25px;
				background: #000;
				position: absolute;
				left: 0;
				bottom: 0;
				filter: alpha(opacity=70);
				-moz-opacity: 0.7;
				opacity: 0.7;
			}
			.t {
				display: block;
				line-height: 25px;
				position: absolute;
				left: 0;
				right: 0;
				bottom: 0;
				color: #fff;
				font-style: normal;
				font-weight: bold;
				text-align: left;
				padding: 0 8px;
				span {
					color: #aaa;
					font-weight: normal;
					float: right;
				}
			}
		}
	}
	.list-photo {
		list-style: none;
		margin: 0px;
		padding: 0px;
		li {
			.thumb {
				display: block;
				img {
					width: 210px;
					height: 130px;
				}
			}
			.title {
				display: block;
				text-align: center;
				height: 30px;
				text-decoration: none;
				line-height: 30px;
				overflow: hidden;
			}
			.summary {
				color: #aaaaaa;
				text-align: center;
			}
		}
	}
	.list-hot {
		margin: 5px 15px 15px;
		padding: 0px;
		list-style-image: url('li-arrow.gif');
		li {
			line-height: 25px;
			a {
				width: 250px;
				font-size: 12px;
				color: #666;
				word-break: keep-all;
				white-space: nowrap;
				overflow: hidden;
				text-overflow: ellipsis;
			}
		}
	}
	.list-brand {
		list-style: none;
		margin: 0px;
		padding: 0px;
		li {
			.thumb {
				display: block;
				img {
					width: 204px;
					height: 94px;
					border: 3px #e5e5e5 solid;
				}
			}
			.title {
				display: block;
				text-align: center;
				height: 35px;
				line-height: 30px;
				overflow: hidden;
				color: #666;
			}
		}
	}
	.list-news {
		margin-bottom: 10px;
		.photo {
			img {
				width: 210px;
				height: 120px;
			}
			
		}
		.headline {
			line-height: 20px;
			border-bottom: 1px #ccc dashed;
			padding-bottom: 6px;
			margin-bottom: 6px;
			h4 {
				margin: 0px;
				padding: 0px;
				font-size: 14px;
				text-align: center;
				color: #666666;
				line-height: 24px;
			}
			p {
				margin: 0px;
				padding: 0px;
				color: #aaaaaa;
				font-size: 12px;
				font-weight: normal;
			}
		}
		.list-news-more {
			padding: 0px;
			margin: 0px;
			list-style: none;
			li {
				line-height: 20px;
				padding-left: 10px;
				background: url('dot.png') 0px center no-repeat;
			}
		}
	}
	.list-link {
			padding: 0px;
			margin: 0 0 9px;
			list-style: none;
			li {
				line-height: 20px;
				padding-left: 10px;
				background: url('dot.png') 0px center no-repeat;
				.clearfix();
				.title {
					display: block;
					float: left;
				}
				.more {
					display: block;
					float: right;
				}
			}
	}
	.list-groupon {
		list-style: none;
		margin: -9px 0 0;
		padding: 0px;
		li {
			padding: 9px 0 0;
			.thumb {
				display: block;
				position: relative;
				img {
					width: 210px;
					height: 150px;
				}
				.t {
					display: block;
					line-height: 25px;
					position: absolute;
					left: 0;
					right: 0;
					bottom: 0;
					color: #fff;
					background: rgba(0,0,0,0.8);
					*background: #000000;
					font-style: normal;
					font-weight: bold;
					text-align: center;
					padding: 0 8px;
				}
			}
			.summary {
				margin-top: 5px;
				a {
					text-decoration: none;
				}
			}
			.count {
				margin-top: 5px;
				color: #aaaaaa;
				background: url('ico-light.png') 0px -15px no-repeat;
				padding: 0 0 0 20px;
			}
			.price {
				margin: 5px -9px 0;
				padding: 5px 10px;
				background: #efefef url('bg-linen.png') 0px 0px repeat;
				*zoom: 1;
				.clearfix();
				.o {
					display: block;
					color: #aaaaaa;
					text-decoration: line-through;
					float: left;
					font-size: 14px;
					.cleartype();
				}
				.p {
					display: block;
					clear: left;
					float: left;
					font-size: 30px;
					line-height: 36px;
					font-weight: bold;
				}
				a.btn {
					float: right;
					margin-top: -6px;
					*margin-top: 12px;
				}
			}
		}
	}
	.list-groupon-sider {
		list-style: none;
		margin: -9px 0 0;
		padding: 0px;
		li {
			padding: 9px 0 0;
			.thumb {
				display: block;
				position: relative;
				img {
					width: 260px;
					height: 150px;
				}
				.t {
					display: block;
					line-height: 25px;
					position: absolute;
					left: 0;
					right: 0;
					bottom: 0;
					color: #fff;
					background: rgba(0,0,0,0.8);
					*background: #000000;
					font-style: normal;
					font-weight: bold;
					text-align: center;
					padding: 0 8px;
				}
			}
			.summary {
				margin-top: 5px;
				a {
					text-decoration: none;
				}
			}
			.count {
				margin-top: 5px;
				color: #aaaaaa;
				background: url('ico-light.png') 0px -15px no-repeat;
				padding: 0 0 0 20px;
			}
			.price {
				margin: 5px -9px 0;
				padding: 5px 10px;
				background: #efefef url('bg-linen.png') 0px 0px repeat;
				*zoom: 1;
				.clearfix();
				.o {
					display: block;
					color: #aaaaaa;
					text-decoration: line-through;
					float: left;
					font-size: 14px;
					.cleartype();
				}
				.p {
					display: block;
					clear: left;
					float: left;
					font-size: 30px;
					line-height: 36px;
					font-weight: bold;
				}
				a.btn {
					float: right;
					margin-top: -6px;
					*margin-top: 12px;
				}
			}
		}
	}
	.list-mvp-post {
		margin: -9px 0 0;
		padding: 0px;
		list-style: none;
		.user-detail {
			padding: 9px 0;
			font-weight: normal;
			.avatar {
				float: left;
				img {
					width: 50px;
					height: 50px;
					border-radius: 3px;
				}
			}
			.name {
				margin: 0 0 0 60px;
				padding: 0px;
				line-height: 20px;
				font-size: 14px;
			}
			.count,
			.type {
				margin: 0 0 0 60px;
				line-height: 15px;
				color: #666666;
			}
		}
		.posts {
			margin: 0px;
			padding: 0px;
			.list-post {
				margin: 0px;
				padding: 0px;
				list-style: none;
				li {
					padding: 9px 0;
					border-top: 1px dotted #d6d6d6;
					.clearfix();
					.content {
						color: #aaaaaa;
						a {
							margin: 0 5px;
						}
						p {
							display: inline;
							color: #666666;
							a {
								text-decoration: none;
							}
						}
					}
					.time {
						color: #aaaaaa;
						float: left;
					}
					.count {
						color: #aaaaaa;
						float: right;
					}
				}
			}
		}
	}
	.list-mvp {
		margin: -9px 0 0;
		padding: 0px;
		list-style: none;
		.user-detail {
			background: url('arrow-top.png') 20px bottom no-repeat;
			padding: 9px 0;
			font-weight: normal;
			.avatar {
				float: left;
				img {
					width: 50px;
					height: 50px;
					border-radius: 3px;
				}
			}
			.name {
				margin: 0 0 0 60px;
				padding: 0px;
				line-height: 20px;
				font-size: 14px;
			}
			.count,
			.type {
				margin: 0 0 0 60px;
				line-height: 15px;
				color: #666666;
			}
		}
		.post {
			margin: 0 -9px;
			background-color: #f2f2f2;
			padding: 9px;
			.clearfix();
			.content {
				color: #aaaaaa;
				a {
					margin: 0 5px;
				}
				p {
					display: inline;
					color: #666666;
					a {
						text-decoration: none;
					}
				}
			}
			.time {
				color: #aaaaaa;
				float: left;
			}
			.count {
				color: #aaaaaa;
				float: right;
			}
		}
	}
	.list-user {
		ul {
			list-style: none;
			margin: 0 0 5px -4px;
			padding: 0 0 5px 0;
			display: inline-block;
			border-bottom: 1px dotted #d9d9d9;
		}
		li{
			float: left;
			margin: 0 0 5px 7px;
			img {
				height:30px;
				width:30px;
			}
		}
		.interact {
			text-align: right;
			color: #666;
			margin: 5px 0;
			line-height: 30px;
			a {
				color: #666;
				font-size: 12px;
				margin: 0 5px;
			}
		}
	}
	.feedroll {
		position: relative;
		height: 255px;
		margin: -8px 0 10px;
		overflow: hidden;
		.list-feed {
			position: absolute;
			margin: 0px;
			padding: 0px;
			list-style: none;
			z-index: 1;
			li {
				padding: 8px 0;
				color: @grayLight;
				border-bottom: 1px dotted #d6d6d6;
				.clearfix();
				a {
					margin: 0 5px;
				}
				.avatar {
					display: inline-block;
					vertical-align: middle;
					margin: -2px 0 0;
					img {
						width: 20px;
						height: 20px;
						border-radius: 2px;
					}
				}
			}	
		}
		.mask {
			position: absolute;
			bottom: 0;
			width: 100%;
			height: 20px;
			font-size: 1px;
			line-height: 1px;
			z-index: 5;
			background: url('mask_feedroll.png') repeat-x;
		}
	}
}
