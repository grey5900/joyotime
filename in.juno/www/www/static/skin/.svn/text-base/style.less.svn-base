#returntop {
	display: none;
	cursor: pointer;
	position: fixed;
	right: 50%;
	margin-right: -580px;
	bottom: 20px;
	width: 50px;
	height: 50px;
	text-indent: -9999px;
	overflow: hidden;
	background: url(returntop.png) 0px 0px no-repeat;
	_background: url(returntop.gif) 0px 0px no-repeat;
	&:hover {
		background-position: 0px -50px;
	}
}
#messager {
	min-width: 200px;
	max-width: 300px;
	_width: 200px;
	padding: 20px 15px;
	line-height: 20px;
	overflow: hidden;
	position: absolute;
	z-index: 9999;
	text-align: center;
	font-size: 12px;
	font-weight: bold;
	background-color: #ffffff;
	border: 1px solid rgba(0,0,0,.2);
	border: 1px solid #666 ~"\9";
	border-radius: 3px;
	.box-shadow(0 1px 3px rgba(0,0,0,.3));
	.opacity(0);
}


.placeico {
	background-image: url('ico.png');
	background-repeat: no-repeat;
	background-position: -191px 0;
	width: 11px;
	height: 13px;
	display: inline-block;
	.ie7-inline-block();
	margin-left: 5px;
	vertical-align: middle;
	overflow: hidden;
}

.placedico {
	background-image: url('ico-light.png');
	background-repeat: no-repeat;
	background-position: -1px -2px;
	width: 12px;
	height: 13px;
	display: inline-block;
	.ie7-inline-block();
	margin-right: 5px;
	vertical-align: middle;
	overflow: hidden;
}
.peopleico {
	background-image: url('ico-light.png');
