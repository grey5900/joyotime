<!--{template web/header}-->
<div class="container" id="container">
	
	<!--{template f_header}-->
	<div class="user-section">
		<div class="section-header"></div>
		<div class="section-content">
			<div class="info">
				<a class="avatar avatar_box pull-left"><img src="{$info[avatar]}"/></a>
				<div class="detail">
					<i class="pull-right ico-integra whiteText font-yab"><h3>积分：<em>{$info[point]}</em></h3></i>
					<h3 class="greenText font-yab">{$nickname}</h3>
					<ul>
						<li><span>签到：<em class="redText">{$info[checkinCount]}</em></span></li>
						<li><span>点评：<em class="redText">{$info[tipCount]}</em></span></li>
						<li><span>照片：<em class="redText">{$info[photoCount]}</em></span></li>
						<li><span>收藏：<em class="redText">{$info[favoriteCount]}</em></span></li>
					</ul>
					<p>{$info[description]}</p>					
				</div>
			</div>
			<div class="info-table">
					<table>
						<tr>
							<td><span>性别：</span><em>{$info[gender]}</em></td>
							<td><span>年龄：</span><em>{$info[age]}</em></td>
						</tr>
						<tr>
							<td><span>星座：</span><em>{$info[star]}</em></td>
							<td><span>最后登录：</span><em>{$info[last_signin]}</em></td>
						</tr>
					</table>
				</div>
		</div>
		<div class="section-footer"></div>
	</div>
</div>
<!--{template web/cur_event}-->

<!--{template web/footer}-->