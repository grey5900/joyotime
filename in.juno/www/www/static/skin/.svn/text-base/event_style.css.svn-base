.ads {
  margin-bottom: 15px;
}
.ads .ads-slide {
  zoom: 1;
  margin: 0 auto;
  width: 1000px;
}
.ads .ads-slide .carousel-inner {
  margin-bottom: 15px;
  list-style: none;
  margin: 0;
  width: 1000px;
  height: 320px;
  overflow: hidden;
}
.ads .ads-slide .carousel-inner img {
  width: 1000px;
  height: 320px;
  display: block;
}
.ads .ads-slide .carousel-inner .carousel-caption {
  text-align: left;
  padding: 0 10px;
  margin-bottom: 20px;
}
.ads .ads-slide .carousel-inner .carousel-caption h3 {
  font-size: 18px;
  line-height: 18px;
  margin: 15px 0;
  float: left;
  height: 18px;
  overflow: hidden;
}
.ads .ads-slide .carousel-inner .carousel-caption p {
  font-size: 14px;
  line-height: 18px;
  margin: 15px 0;
  float: right;
}
.list-content {
  background: #fff;
  border: 1px #e6e6e6 solid;
  overflow: hidden;
}
.level {
  *zoom: 1;
  overflow: hidden;
  width: 100%;
}
.level:before,
.level:after {
  display: block;
  content: "";
  line-height: 0;
}
.level:after {
  clear: both;
}
.level .main-right {
  float: right;
  width: 280px;
}
.level .main-left {
  float: left;
  width: 80px;
}
.level .main-middle {
  float: left;
  margin-left: 20px;
  width: 600px;
}
.mold {
  background: #fff;
  border: 1px solid #e6e6e6;
  border-top-width: 0;
}
.event-list {
  display: inline-block;
  *display: inline;
  /* IE7 inline-block hack */

  *zoom: 1;
  padding: 15px;
}
.event-list .media .pull-left img {
  width: 300px;
  height: 120px;
}
.event-list .media .media-body h4 {
  font-size: 16px;
  color: #333;
  background: url("dotted_line.gif") repeat-x scroll 0 bottom transparent;
  padding: 0 0 10px;
  text-align: center;
  margin: 0;
}
.event-list .media .media-body p {
  display: block;
  line-height: 20px;
  font-size: 12px;
  margin-top: 15px;
}
.event-list .media .media-body .info {
  color: #aaa;
  float: right;
  display: inline-block;
}
.event-list .media .media-body .time {
  color: #666;
  float: left;
  display: inline-block;
}
.blankfix20 {
  height: 20px;
  font-size: 1px;
  border-top: 1px #f0f0f0 solid;
}
/* 日历 */
.calendar {
  background: url("calendar.png") 0 0 no-repeat;
  width: 80px;
  height: 89px;
  display: block;
  text-align: center;
}
.calendar .month-show {
  color: #fff;
  font-size: 12px;
  line-height: 18px;
  padding-top: 5px;
  font-weight: bold;
}
.calendar .day-show {
  color: #444;
  font-size: 36px;
  line-height: 18px;
  padding-top: 20px;
  font-family: Georgia;
}
.event-desc {
  border-top: 2px solid #96292B;
}
.event-desc .ads {
  padding: 10px 9px;
  width: 580px;
}
.event-desc .ads img {
  width: 580px;
  height: 230px;
}
.event-desc .event-intro {
  margin: 10px 9px;
  width: 580px;
  padding: 0 0 20px 0;
  font-size: 14px;
  line-height: 22px;
  color: #666;
  border-bottom: 1px dotted #d9d9d9;
  word-break: normal;
  word-wrap: break-word;
}
.event-desc .event-intro .name {
  font-size: 16px;
  font-weight: bold;
}
.event-desc .event-intro .enter {
  float: right;
}
.event-desc .event-intro .time {
  color: #aaa;
  font-size: 12px;
}
.event-desc .event-intro .title {
  color: #d63333;
  font-weight: bold;
}
.event-desc .event-intro p {
  text-indent: 2em;
}
.event-desc .event-intro dl {
  *zoom: 1;
}
.event-desc .event-intro dl:before,
.event-desc .event-intro dl:after {
  display: block;
  content: "";
  line-height: 0;
}
.event-desc .event-intro dl:after {
  clear: both;
}
.event-desc .event-intro dl dt {
  width: 100px;
  float: left;
}
.event-desc .event-intro dl dd {
  margin-left: 100px;
}
#place-map {
  margin-bottom: 9px;
  cursor: pointer;
}
.dynamic {
  border-top: 2px solid #96292B;
}
.dynamic .tit {
  padding: 5px 10px;
}
#modal-event-enter .modal-body {
  padding: 20px 50px 0 0;
}
.indent .show-full {
  font-size: 12px;
  color: #D63333;
  text-decoration: none;
}
.indent .show-full:hover {
  background: #D63333;
  color: #fff;
}
